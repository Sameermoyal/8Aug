// 1.  i/p:  [1 2 3 4 5 6]
//   o/p:   1 3 6 10 15 21 


#include<bits/stdc++.h>
using namespace std;

int main(){
   int add=0;
   
   vector<int>arr ={1 ,2 ,3 ,4 ,5, 6};
   
   for(int i=0;i<arr.size();i++){
       add=arr[i]+add;
       arr[i]=add;
   }
   for(auto it:arr){
       cout<<it<<"  ";
   }
}